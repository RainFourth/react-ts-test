{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],

        // allow resolve src folder as module. You can use: import LoadingIc from "src/components/SvgIcons/LoadingIc"
        "baseUrl":".",

        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "downlevelIteration": true,
        "noImplicitAny": false,
        "noImplicitOverride": true,
        "allowUnreachableCode": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",


        /*"baseUrl": "./src",
        "paths": {
            "@ic*//*": ["./assets/icons*//*"],
            "@im*//*": ["./assets/images*//*"],
        },*/


    },
    "include": [
        "src"
    ],
}
